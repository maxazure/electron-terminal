{
  "name": "electron-terminal",
  "version": "1.0.0",
  "description": "A terminal emulator built with Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "npx electron-rebuild",
    "build": "electron-builder",
    "postinstall": "npx electron-rebuild"
  },
  "keywords": [
    "terminal",
    "emulator",
    "electron",
    "node-pty"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "electron": "^29.0.1",
    "electron-builder": "^24.13.1",
    "express": "^4.21.2",
    "node-pty": "^0.10.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "appId": "com.electron.terminal",
    "productName": "Electron Terminal",
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
